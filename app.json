{
  "expo": {
    "name": "Universal S3 client",
    "slug": "universal-s3-client",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/aws_buckets3.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/aws_buckets3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vincentventalon.universals3client",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Universal S3 Client needs access to your photo library so you can select and upload images or files to your S3 storage. For example, you can choose a photo to upload to your bucket."
      },
      "entitlements": {
        "keychain-access-groups": [
          "$(AppIdentifierPrefix)group.com.vincentventalon.universals3client.shared"
        ]
      }
    },
    "macos": {
      "bundleIdentifier": "com.vincentventalon.universals3client",
      "category": "public.app-category.utilities"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/aws_buckets3.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/aws_buckets3.png"
    },
    "extra": {
      "eas": {
        "projectId": "66911a74-ef2b-4192-bfac-92067ac83b06"
      }
    },
    "owner": "vincentventalon"
  }
}
